 CREATE TABLE EMPLOYEE(
EMP_ID VARCHAR (8) NOT NULL PRIMARY KEY,
FIRST_NAME VARCHAR (15),
SURNAME VARCHAR (15),
CONTACT_NUMBER INT,
DEPARTMENT VARCHAR (10),
ADDRESS VARCHAR (255),
EMAIL VARCHAR (20)
);
