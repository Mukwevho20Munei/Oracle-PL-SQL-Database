CREATE TABLE CUSTOMER(
CUSTOMER_ID SMALLINT NOT NULL PRIMARY KEY,
FIRST_NAME VARCHAR (10),
SURNAME VARCHAR (10),
ADDRESS VARCHAR (255),
CONTACT_NUMBER INT,
EMAIL VARCHAR (20)
);
